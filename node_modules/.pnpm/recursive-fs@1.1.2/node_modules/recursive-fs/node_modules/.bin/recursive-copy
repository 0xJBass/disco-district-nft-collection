#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/justinbassey/Desktop/disco-district-nft-collection/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/justinbassey/Desktop/disco-district-nft-collection/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/recursive-copy" "$@"
else
  exec node  "$basedir/../../bin/recursive-copy" "$@"
fi
